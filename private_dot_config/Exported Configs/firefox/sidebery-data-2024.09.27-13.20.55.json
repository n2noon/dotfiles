  "sidebarCSS": "#root.root {--tabs-color-layer-opacity: 0.08;}\n#root.root {--tabs-activated-color-layer-opacity: 0.3;}\n#root.root {--nav-btn-width: 32px;}\n#root.root {--nav-btn-height: 32px;}\n#root.root {--tabs-pinned-height: 33px;}\n#root.root {--tabs-pinned-width: 28px;}\n#root.root {--tabs-height: 27px;}\n#root.root {--tabs-indent: 8px;}\n#root.root {--tabs-font: 12.4px JetBrains Mono;}\n\n\n/**\n* Two lined tabs\n*/\n#root {\n  --tabs-height: 31px !important;\n  --tabs-title-padding: 4px;\n  --tabs-font-size: .8rem;\n  --tabs-title-lines: 2;\n}\n\n.Tab .t-box {\n  align-items: center;\n  max-height: calc(var(--tabs-height) - var(--tabs-title-padding));\n  overflow: hidden;\n}\n\n.Tab .title {\n  font-size: var(--tabs-font-size);\n  white-space: pre-wrap;\n  line-height: calc((var(--tabs-height) - var(--tabs-title-padding)) / var(--tabs-title-lines));\n}",
