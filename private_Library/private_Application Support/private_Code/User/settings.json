{
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "editor.formatOnSave": true
    },
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.intelliSenseEngineFallback": "enabled",
    "clangd.path": "/Users/kalk/Library/Application Support/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/18.1.3/clangd_18.1.3/bin/clangd",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.showOptionsMovedNotification": false,
    "debug.allowBreakpointsEverywhere": true,
    "editor.accessibilitySupport": "off",
    // "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "editor.fontFamily": "Iosevka Fixed, Commit Mono, JetBrains Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.fontLigatures": "'cv10' 19, 'cv01' 3, 'cv05' 5, 'cv08' 2, 'cv41' 11, 'cv44' 6, 'cv45' 6, 'cv47' 4, 'cv46' 1, 'cv48' 8, 'cv49' 8, 'cv54' 7, 'cv55' 8, 'cv26' 4, 'VSAO' 1",
    "workbench.fontAliasing": "auto",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.minimap.enabled": false,
    "editor.rulers": [
        120
    ],
    "editor.unicodeHighlight.invisibleCharacters": false,
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info",
        "hint"
    ],
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.ignoreRebaseWarning": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "jsonc": true
    },
    "lldb.suppressUpdateNotifications": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "security.workspace.trust.untrustedFiles": "open",
    // "terminal.external.osxExec": "wezterm.app",
    "terminal.integrated.enableImages": true,
    "terminal.integrated.fontFamily": "Iosevka Fixed",
    // "editor.fontWeight": "500",
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.inheritEnv": false,
    "totalTypeScript.hiddenTips": [
        "optional-object-property",
        "interface-declaration",
        "literal-type",
        "union-type",
        "type-alias-declaration",
        "basic-types",
        "passing-generics-to-types",
        "variable-type-annotation",
        "typing-function-parameters",
        "array-type",
        "non-null-expression",
        "ts-object-type",
        "function-return-type"
    ],
    "totalTypeScript.hideAllTips": false,
    "totalTypeScript.hideBasicTips": false,
    "vim.neovimUseConfigFile": true,
    "vim.smartRelativeLine": true,
    // "vim.sneak": true,
    // "vim.sneakReplacesF": true,
    "vim.useCtrlKeys": true,
    "vim.highlightedyank.enable": true,
    "vim.smartcase": true,
    "vim.startofline": true,
    // "vim.targets.enable": true,
    "vim.surround": true,
    "vim.useSystemClipboard": true,
    "window.commandCenter": false,
    "window.newWindowDimensions": "maximized",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000"
    },
    "workbench.colorTheme": "Gruvbox Material Dark",
    "workbench.editor.showTabs": "none",
    "workbench.layoutControl.enabled": false,
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.advanced": {
    },
    "github.copilot.selectedCompletionModel": "",
    "continue.telemetryEnabled": false
}